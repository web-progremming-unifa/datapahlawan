{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "headers": [
      {
        "source": "/heroes.json",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=0, stale-while-revalidate"
          }
        ]
      }
    ]
  }